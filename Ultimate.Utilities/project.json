{
  "version": "1.0.0-*",
  "title": "Ultimate.Utilities",
  "description": "JsonConfig.Core is a popular high-performance JSON framework for .NET",
  "language": "en-US",
  "author": "Satish Kumar B",
  "packOptions": {
    "projectUrl": "http://www.newtonsoft.com/json",
    "tags": [ "json" ],
    "licenseUrl": "https://raw.github.com/JamesNK/Newtonsoft.Json/master/LICENSE.md",
    "iconUrl": "http://www.newtonsoft.com/content/images/nugeticon.png",
    "repository": {
      "type": "git",
      "url": "git://github.com/JamesNK/Newtonsoft.Json"
    }
  },
  "frameworks": {
    "net45": {
      "dependencies": {
        "Newtonsoft.Json": "9.0.1"
      }
    },
    "netstandard1.6": {
      "imports": [ "dnxcore50", "net45" ],
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "Newtonsoft.Json": "9.0.1",
        "System.Globalization.Extensions": "4.0.1",
        "System.Linq.Parallel": "4.0.0.0",
        "System.Reflection.TypeExtensions": "4.1.0"
      }
    }
  },
  "scripts": {
    "postcompile": [
      "dotnet pack --no-build --configuration %compile:Configuration%"
    ]
  },
  "dependencies": {
    "System.Linq.Queryable": "4.0.1"
  }
}
